{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "shell",
      "command": "pnpm run build",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "watch",
      "type": "shell",
      "command": "pnpm run watch",
      "group": {
        "kind": "build"
      }
    },
    {
      "label": "clean",
      "type": "shell",
      "command": "rm -rf dist",
      "group": {
        "kind": "build"
      }
    }
  ]
}
